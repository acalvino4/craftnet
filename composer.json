{
  "require": {
    "adam-paterson/oauth2-stripe": "^2.0.1",
    "aws/aws-sdk-php": "^3.233",
    "craftcms/aws-s3": "2.0.1",
    "craftcms/cms": "4.3.x-dev#18861cea541350ade977ea017405c2471f6f9d56 as 4.3.0-alpha",
    "craftcms/commerce": "4.1.0",
    "craftcms/commerce-stripe": "3.0.1",
    "craftcms/postmark": "3.0.0",
    "craftcms/webhooks": "3.0.4",
    "ddeboer/vatin": "^2.2.2",
    "jeremykendall/php-domain-parser": "^6.1.1",
    "justinrainbow/json-schema": "^5.2.12",
    "knplabs/github-api": "^3.8",
    "league/oauth2-github": "^2.0.0",
    "league/oauth2-server": "^6.1.1",
    "mead-steve/mono-snag": "^4.1",
    "moccalotto/eu": "^0.3.0",
    "omnilight/yii2-scheduling": "^1.1.6",
    "nystudio107/craft-twigpack": "4.0.0-beta.4",
    "paypal/rest-api-sdk-php": "^1.14",
    "stevenmaguire/oauth2-bitbucket": "^2.0",
    "tecnickcom/tcpdf": "^6.5",
    "thamtech/yii2-ratelimiter-advanced": "^0.5",
    "yiisoft/yii2-redis": "^2.0.17",
    "logtail/monolog-logtail": "^0.1.3",
    "vlucas/phpdotenv": "^5.4.1"
  },
  "require-dev": {
    "codeception/codeception": "^4.2.1",
    "codeception/module-asserts": "^1.3.1",
    "codeception/module-datafactory": "^1.1",
    "codeception/module-phpbrowser": "^1.0.3",
    "codeception/module-rest": "^1.4.2",
    "codeception/module-yii2": "^1.1.5",
    "craftcms/phpstan": "dev-main",
    "craftcms/rector": "dev-main",
    "yiisoft/yii2-shell": "^2.0.4"
  },
  "autoload": {
    "psr-4": {
      "craftnet\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "platform": {
      "php": "8.0.8"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "scripts": {
    "phpstan": "phpstan --memory-limit=1G"
  }
}
