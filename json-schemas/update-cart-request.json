{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "email": {"type": "string", "format": "email"},
    "billingAddress": {
      "type": "object"
    },
    "items": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": {"enum": ["cms-edition", "cms-renewal", "plugin-edition", "plugin-renewal"]},
          "note": {"type": ["string", "null"]}
        },
        "required": ["type"]
      }
    },
    "couponCode": {"type": ["string", "null"]}
  }
}
